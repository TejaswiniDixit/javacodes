import java.util.*;

class ListCopier {
    public static void copyList(List<? super Number> dest, List<? extends Number> src) {
        for (Number n : src) {
            dest.add(n);
        }
    }
}

public class CopyMain {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        List<Integer> srcList = new ArrayList<>();
        System.out.print("Enter number of elements in source list: ");
        int n = sc.nextInt();
        System.out.println("Enter " + n + " integers:");
        for (int i = 0; i < n; i++) {
            srcList.add(sc.nextInt());
        }

        List<Number> destList = new ArrayList<>();
        ListCopier.copyList(destList, srcList);

        System.out.println("Source List: " + srcList);
        System.out.println("Destination List after copy: " + destList);

        sc.close();
    }
}
