import java.util.*;

class NumericSum {
    public static double sumNumbers(List<? extends Number> list) {
        double sum = 0.0;
        for (Number n : list) {
            sum += n.doubleValue();
        }
        return sum;
    }
}

public class SumMain {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        List<Integer> intList = new ArrayList<>();
        System.out.print("Enter number of integers: ");
        int n1 = sc.nextInt();
        System.out.println("Enter " + n1 + " integers:");
        for (int i = 0; i < n1; i++) {
            intList.add(sc.nextInt());
        }

        List<Double> doubleList = new ArrayList<>();
        System.out.print("Enter number of doubles: ");
        int n2 = sc.nextInt();
        System.out.println("Enter " + n2 + " doubles:");
        for (int i = 0; i < n2; i++) {
            doubleList.add(sc.nextDouble());
        }

        System.out.println("Sum of integers: " + NumericSum.sumNumbers(intList));
        System.out.println("Sum of doubles: " + NumericSum.sumNumbers(doubleList));

        sc.close();
    }
}
